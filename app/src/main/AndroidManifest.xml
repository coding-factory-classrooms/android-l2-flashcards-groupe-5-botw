<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.aph.flashcard_botw">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/botw_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/botw_logo_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.FlashCardBOTW">
        <activity android:name=".ResultActivity"></activity>
        <activity android:name=".EnlargedImageActivity" />
        <activity android:name=".AboutActivity" />
        <activity android:name=".QuestionsListActivity"></activity>
        <activity android:name=".FlashCardActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>